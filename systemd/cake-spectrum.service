[Unit]
Description=Adaptive CAKE Binary Search Calibration for Spectrum
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
User=kevin
WorkingDirectory=/home/kevin/wanctl
ExecStart=/usr/bin/python3 /home/kevin/wanctl/adaptive_cake.py --config /home/kevin/wanctl/configs/spectrum_binary_search.yaml
StandardOutput=journal
StandardError=journal
SyslogIdentifier=cake-spectrum-binary

[Install]
WantedBy=multi-user.target
